  font-size: 13px;
  color: #333;
  line-height: 30px;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  text-align: left;
  border-left: none !important;
  padding: 0 7px;
}
