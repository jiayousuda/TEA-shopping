/*产品  */
.pro-con {
  float: left;
  width: 46%;
  padding:0 2%;
}

.product_con {
  overflow: hidden;
  /* padding: 14px 0; */
}
.con-con {
  width: 100%;
  display: inline-block;
  background: #f4f4f4;
  border:1px solid rgba(185, 136, 55, 0.3);
}

.imm {
  display: block;
  width: 100px;
  height: 100px;
  border-radius: 50%;
  margin: 27px auto 10px auto;
}

.tit-top {
  color: #9b9b9b;
  font-size: 16px;
  text-align: center;
}

.con-con image {
  width: 100%;
  height: 142px;
  border-radius: 5px;
}

.pro-font {
  font-size: 13px;
  color: #333;
  line-height: 30px;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  text-align: left;
  border-left: none !important;
  padding: 0 7px;
}
.price_buy{
  display: flex;
  padding:18px 0 0 0;
}
.price {
  flex: 1;
  font-size: 15px;
  color:#B28232;
  border-left: none !important;
  padding: 0 7px;
}
.buy_con{
  flex:0.8;
   position: relative;
  top: -3px;
  right: 6px; 
  text-align: center;
  height: 25px;
  line-height: 25px;
}
.buy_con image{
  width: 100%;
  height: 100%;
}
.buy_con text{
  font-size:12px;
position:absolute;
top:2rpx;
