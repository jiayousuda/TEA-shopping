
.his_tit{
  height:40px;
line-height:40px;
  font-size: 14px;
  color: #333;
  position: relative;
  background: #f5f5f5;
}
.tit_left{
position: absolute;
left:10px;
}
.tit_right{
position: absolute;
right:10px;
}


.his_li{
  display: flex;
  padding: 5px 10px;
  border-bottom: 1px solid #f5f5f5;
  line-height: 23px;
}
.li_left{
  flex: 1;
  text-align: left;
}
.li_right{
  flex: 1;
  text-align: right;
}
.his_money{
  font-size: 14px;
  color: #333;
}
.chongzhi,.his_resule,.his_time{
  font-size: 12px;
  color: #666;
}
.nomore{
  font-size:14px;
  background:#f1f1f1;
  color:#999;
  line-height:25px;
  text-align:center;
  margin-top: -10px;
}
