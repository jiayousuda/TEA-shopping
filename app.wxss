/* 标题带线 */
.title-line {
  line-height: 40px;
  text-align: center;
  position: relative;
}
.title-line::after {
  content: '';
  border-top: 1px solid #ebebeb;
  width: 35%;
  position: absolute;
  top: 19px;
  display: inline-block;
  margin-left: 5%;
}
.title-line::before {
  content: '';
  border-top: 1px solid #ebebeb;
  width: 35%;
  position: absolute;
  top: 19px;
  display: inline-block;
  left: 5px;
}