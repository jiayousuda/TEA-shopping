  width:100%;
  z-index:99;
  max-height:450px;
}
.pop .dialog{
  position:absolute;
  width:260px;
  z-index:99;
  height:260px;
  background:#fff;
  top:50%;
  left:50%;
  margin-left:-130px;
  margin-top:-130px;
  border-radius:5px;
}
