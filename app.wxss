/* pop */
.pop{
  position:fixed;
  top:0;
  left:0;
  z-index:98;
  width:100%;
  height:100%;
  background:rgba(0,0,0,0.4);
}
.pop .content{
  position:absolute;
  bottom:0;
  left:0;
  width:100%;
  z-index:99;
  max-height:450px;
}

.pop-gg{
  position:fixed;
  top:0;
  left:0;
  bottom: 0px;
   z-index:98; 
  width:100%;
  background:rgba(0,0,0,0.4);
}
.pop-gg .content{
  position:absolute;
  bottom:0;
  left:0;
  width:100%;
  z-index:100;
  max-height:450px;
}
.pop-gg .canl{
  position:absolute;
